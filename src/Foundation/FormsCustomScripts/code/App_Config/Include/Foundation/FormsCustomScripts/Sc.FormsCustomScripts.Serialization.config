<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <unicorn>
      <configurations>
        <configuration name="Foundation.Sc.UpdateContact" description="Forms Custom Scripts">
          <targetDataStore physicalRootPath="$(SourceFolderScUpdateContact)\src\Foundation\FormsCustomScripts\serialization" type="Rainbow.Storage.SerializationFileSystemDataStore, Rainbow" useDataCache="false" singleInstance="true" />
          <predicate>
            <include name="Templates" database="master" path="/sitecore/templates/Foundation/Forms Custom Script">
            </include>
            <include name="TemplatesForms" database="master" path="/sitecore/templates/System/Forms/Fields/Button">
            </include>
            <include name="SystemForms" database="master" path="/sitecore/system/Settings/Forms/Field Types/Structure/Submit Button">
            </include>
          </predicate>
        </configuration>
      </configurations>
    </unicorn>
  </sitecore>
</configuration>